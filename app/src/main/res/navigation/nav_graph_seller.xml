        tools:layout="@layout/fragment_seller_orders" />

    <fragment
        android:id="@+id/nav_add_product"
        android:name="com.imersa.warnu.ui.seller.product.AddProductFragment"
        android:label="Add Product"
        tools:layout="@layout/fragment_add_product" />

    <fragment
        android:id="@+id/nav_edit_product"
        android:name="com.imersa.warnu.ui.seller.product.EditProductFragment"
        android:label="Edit Product"
        tools:layout="@layout/fragment_edit_product" />

    <fragment
        android:id="@+id/nav_product_detail"
        android:name="com.imersa.warnu.ui.seller.product.DetailProductFragment"
        android:label="Product Detail"
        tools:layout="@layout/fragment_detail_product" />

    <fragment
        android:id="@+id/nav_edit_profile_seller"
        android:name="com.imersa.warnu.ui.seller.profile.EditProfileSellerFragment"
        android:label="Edit Profile"
        tools:layout="@layout/fragment_edit_profile_seller" />

    <fragment
        android:id="@+id/nav_detail_order"
        android:name="com.imersa.warnu.ui.seller.order.DetailOrderFragment"
        android:label="Order Detail"
        tools:layout="@layout/fragment_detail_order" />



