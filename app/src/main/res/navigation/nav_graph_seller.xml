<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_seller"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.imersa.warnu.ui.seller.home.HomeSellerFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_home_seller" />

    <fragment
        android:id="@+id/nav_manage_product"
        android:name="com.imersa.warnu.ui.seller.product.EditManagerFragment"
        android:label="Manage Product"
        tools:layout="@layout/fragment_edit_manager" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.imersa.warnu.ui.seller.profile.ProfileSellerFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile_seller" />

    <fragment
        android:id="@+id/nav_order"
        android:name="com.imersa.warnu.ui.seller.order.OrderManagerFragment"
